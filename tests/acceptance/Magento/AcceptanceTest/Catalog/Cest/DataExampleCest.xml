<?xml version="1.0" encoding="utf-8"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../../magento2-acceptance-test-framework/src/Magento/AcceptanceTestFramework/Test/etc/testSchema.xsd">
    <cest name="dataExampleCest">
        <test name="dataExampleTest" >
            <createData entity="AdminCreateSimpleProductCestCategory" mergeKey="createCategoryKey"/>
            <entity name="myEntity" type="customAttribute" mergeKey="myEntityKey">
                <data key="attribute_code">categoryIds</data>
                <data key="value">$createCategoryKey.id$</data>
            </entity>
            <createData entity="myProduct" mergeKey="createMyProductKey">
                <required-entity name="myEntity"/>
            </createData>
        </test>
    </cest>

</config>